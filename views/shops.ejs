<%-include("partials/header")%>
<h1>This is the shops page!</h1>
<div class="container">
	<div class="row">
		<%shops.forEach(function(shops){%>
		<div class="col-md-4 col-sm-6">
			<div class="card text-center" style="width: auto; margin-bottom: 4px;">
				<img class="card-img-top" src="https://cdn.pixabay.com/photo/2016/01/27/04/32/books-1163695_960_720.jpg"
					alt="Shop Image">
				<div class="car-body" style="padding: 8px;">
					<h4 class="card-title"><%= shops.name %></h4>
					<p><%=shops.id%></p>
					<a href="#" class="btn btn-primary">Pay Here!</a>
				</div>
			</div>
		</div>
		<%});%>
	</div>
</div>

